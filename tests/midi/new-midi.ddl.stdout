new-midi.Block ?a4 (n : uint 64) (P : Grammar ?a4) : Grammar ?a4 =
  do (cur : Stream) <- CurrentStream
     do (_113 : Stream) <- StreamLen n cur
        SetStream _113
     ($$ : ?a4) <- P
     do (_114 : Stream) <- StreamOff n cur
        SetStream _114
     pure $$
 
new-midi.Only ?a1 (P : Grammar ?a1) : Grammar ?a1 =
  do ($$ : ?a1) <- P
     END
     pure $$
 
new-midi._Block ?a4 (n : uint 64) (_P : Grammar {}) : Grammar {} =
  do (cur : Stream) <- CurrentStream
     do (_113 : Stream) <- StreamLen n cur
        SetStream _113
     _P
     (_114 : Stream) <- StreamOff n cur
     SetStream _114
 
new-midi._Only ?a1 (_P : Grammar {}) : Grammar {} =
  do _P
     END
 
new-midi.Chunk ?a8 (name : [uint 8]) (Body : Grammar ?a8) : Grammar ?a8 =
  do new-midi._Block [uint 8] 4 (new-midi._Only [uint 8] (@MatchBytes name))
     ($$ : ?a8) <- do (_117 : uint 64) <- do (_116 : uint 32) <- new-midi.BE32
                                             pure (_116 as uint 64)
                      new-midi.Block ?a8 _117 (new-midi.Only ?a8 Body)
     pure $$
 
