     case _145 is
       { {| ModeMessagesHeader = header |} -> do new-midi._ModeMessage
                                                 @Many[] new-midi._Delta new-midi.ModeMessage new-midi._ModeMessage
       }
 
new-midi._SysEx : Grammar {} =
  Choose biased
    { {- add_f0 -} do @Match {'\240'}
                      (_147 : uint 64) <- new-midi.VarQ
                      new-midi._Block [uint 8] _147 (@Many[] @GetByte)
    | {- as_is -} do @Match {'\247'}
                     (_150 : uint 64) <- new-midi.VarQ
                     new-midi._Block [uint 8] _150 (@Many[] @GetByte)
    }
 
new-midi._MetaSeqSpecific : Grammar {} =
  do Choose biased
       { do @Match {'\NUL'}
            new-midi._BE16
       | @GetByte
       }
     @Many[] @GetByte
 
new-midi._MetaUnknown ?a0 : Grammar {} =
  @Many[] @GetByte
 
new-midi._KeySig : Grammar {} =
  do @GetByte
     (_155 : uint 8) <- GetByte
     @(_155 AS new-midi.Mode)
 
new-midi._SMTPEOffset : Grammar {} =
  do @GetByte
     @GetByte
     @GetByte
     @GetByte
     @GetByte
 
new-midi._TimeSig : Grammar {} =
  do @GetByte
     @GetByte
     @GetByte
     @GetByte
 
new-midi._BE24 : Grammar {} =
  do new-midi._BE16
     @GetByte
 
new-midi._Meta : Grammar {} =
  do @Match {'\255'}
     (tag : uint 8) <- GetByte
     new-midi._Guard (tag <= 127)
     (_159 : uint 64) <- new-midi.VarQ
     new-midi._Block new-midi.Meta _159 (case tag is
                                           { 0 -> new-midi._Only (uint 16) new-midi._BE16
                                           ; 1 -> @Many[] @GetByte
                                           ; 2 -> @Many[] @GetByte
                                           ; 3 -> @Many[] @GetByte
                                           ; 4 -> @Many[] @GetByte
                                           ; 5 -> @Many[] @GetByte
                                           ; 6 -> @Many[] @GetByte
                                           ; 7 -> @Many[] @GetByte
                                           ; 32 -> new-midi._Only (uint 8) @GetByte
                                           ; 47 -> END
                                           ; 81 -> new-midi._Only (uint 24) new-midi._BE24
                                           ; 84 -> new-midi._Only new-midi.SMTPEOffset new-midi._SMTPEOffset
                                           ; 88 -> new-midi._Only new-midi.TimeSig new-midi._TimeSig
                                           ; 89 -> new-midi._Only new-midi.KeySig new-midi._KeySig
                                           ; 127 -> new-midi._Only new-midi.MetaSeqSpecific new-midi._MetaSeqSpecific
                                           ; _ -> do (_175 : new-midi.MetaUnknown (uint 8)) <- new-midi.MetaUnknown (uint 8) tag
                                                     pure {unknown: _175}
                                           })
 
new-midi._Event : Grammar {} =
  Choose biased
    { {- voiceMessage -} new-midi._VoiceMessages
    | {- modeMessage -} new-midi._ModeMessages
    | {- sysEx -} new-midi._SysEx
    | {- meta -} new-midi._Meta
    }
 
new-midi._Track : Grammar {} =
  new-midi._Chunk [new-midi.Delta new-midi.Event] "MTrk" (@Many[] new-midi._Delta new-midi.Event new-midi._Event)
 
new-midi._Main : Grammar {} =
  do (header : new-midi.Header) <- new-midi.Header
     @Many[header.track_num] new-midi._Track
