rec value
  new-midi.VarQBuilder (result : int) : Grammar int =
    do (_122 : new-midi.VarQChunk) <- do (_121 : uint 8) <- GetByte
                                         pure (_121 as new-midi.VarQChunk)
       case _122 is
         { {| More = x |} -> new-midi.VarQBuilder (result <# (x.value))
         ; {| Last = x |} -> pure (result <# (x.value))
