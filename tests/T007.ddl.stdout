module T007
 
--- Imports:
 
--- Type defs:
type T007.Sign = Choose { neg: {}
                        ; pos: {}
                        }
 
--- Rules:
 
T007.Sign : Grammar T007.Sign =
  Choose biased
    { {- pos -} do (_2 : {}) <- @Match {'+'}
                   pure {pos: _2}
    | {- neg -} do (_3 : {}) <- @Match {'-'}
                   pure {neg: _3}
    | {- pos -} do (_4 : {}) <- pure {}
                   pure {pos: _4}
    }
 
T007.SignWithCase : Grammar {} =
  Choose biased
    { do (_5 : uint 8) <- GetByte
         case _5 is
           { 43 -> {- case branch  43 -} pure {}
           ; 45 -> {- case branch  45 -} pure {}
           }
    | pure {}
    }
 
