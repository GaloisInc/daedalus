                            ($$ : D018.Number) <- case sign is
                                                    { {| pos = _ |} -> {- case branch  {| pos = _ |} -} pure n
                                                    ; {| neg = _ |} -> {- case branch  {| neg = _ |} -} do (num : int) <- pure (0 - (n.num))
                                                                                                           (exp : int) <- pure (n.exp)
                                                                                                           pure {num = num,
                                                                                                                 exp = exp}
                                                    }
                            pure $$
 
