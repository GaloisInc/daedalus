module T004
 
--- Imports:
 
--- Type defs:
rec
  type T004.Repeat0 ?a0 = { head: ?a0
                          ; tail: T004.Repeat ?a0
                          }
   
  type T004.Repeat ?a0 = Choose { nil: {}
                                ; cons: T004.Repeat0 ?a0
                                }
 
--- Rules:
 
rec value
  T004.Repeat ?a0 (P : Grammar ?a0) : Grammar (T004.Repeat ?a0) =
    Choose biased
      { {- cons -} do (_4 : T004.Repeat0 ?a0) <- do (head : ?a0) <- P
                                                    (tail : T004.Repeat ?a0) <- T004.Repeat ?a0 P
                                                    pure {head = head,
                                                          tail = tail}
                      pure {cons: _4}
      | {- nil -} do (_5 : {}) <- pure {}
                     pure {nil: _5}
      }
 
T004._Repeat ?a0 (P : Grammar ?a0) (_P : Grammar {}) : Grammar {} =
  Choose biased
    { {- cons -} do _P
                    T004.Repeat ?a0 P
                    pure {}
    | {- nil -} pure {}
    }
