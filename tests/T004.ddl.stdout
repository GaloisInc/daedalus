module T004
 
--- Imports:
 
--- Type defs:
rec
  type T004.Repeat0 ?a2 = { head: ?a2
                          ; tail: T004.Repeat ?a2
                          }
   
  type T004.Repeat ?a2 = Choose { cons: T004.Repeat0 ?a2
                                ; nil: {}
                                }
 
--- Rules:
 
rec value
  T004.Repeat ?a2 (P : Grammar ?a2) : Grammar (T004.Repeat ?a2) =
    Choose fair
      { {- cons -} do (_0 : T004.Repeat0 ?a2) <- do (head : ?a2) <- P
                                                    (tail : T004.Repeat ?a2) <- T004.Repeat ?a2 P
                                                    pure {head = head,
                                                          tail = tail}
                      pure {cons: _0}
      | {- nil -} do (_1 : {}) <- pure {}
                     pure {nil: _1}
      }
 
T004._Repeat ?a2 (P : Grammar ?a2) (_P : Grammar {}) : Grammar {} =
  Choose fair
    { {- cons -} do _P
                    T004.Repeat ?a2 P
                    pure {}
    | {- nil -} pure {}
    }
