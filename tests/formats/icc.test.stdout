    { {- curve -} do (_330 : [uint 16]) <- ICC.CurveType
                     pure {curve: _330}
    | {- parametric_curve -} do (_331 : ICC.ParametricCurveType) <- ICC.ParametricCurveType
                                pure {parametric_curve: _331}
