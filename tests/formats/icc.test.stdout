                                                                  pure {nciexyz_or_pcsxyz: _270}
                                     | {- cielab_or_pcslab -} do (_271 : {}) <- @MatchBytes "Lab "
                                                                 pure {cielab_or_pcslab: _271}
                                     | {- cieluv -} do (_272 : {}) <- @MatchBytes "Luv "
