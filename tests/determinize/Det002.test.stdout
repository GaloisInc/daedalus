proc Simple1() : [Word 8] =
  -- DETERMINIZE HERE
  do _x0 <- match1 { ... }
     case _x0 of
       97 ->
         do x <- do let _x0 = "a"
                    _x1 <- match "b"
                    pure (concat [_x0, _x1])
            pure (concat [x, x])
       98 ->
         do let _x0 = "b"
            _x1 <- match "cde"
            pure (concat [_x0, _x1])
       _ ->
         fail_sys @[Word 8]
 
proc Simple2() : [Word 8] =
  -- DETERMINIZE HERE
  do _x0 <- match1 { ... }
     case _x0 of
       97 ->
         do let _x0 = "a"
            _x1 <- match "bc"
            pure (concat [_x0, _x1])
       98 ->
         do let _x0 = "b"
            _x1 <- match "c"
            pure (concat [_x0, _x1])
       99 ->
         do x <- pure (99 @(Word 8))
            y <- match "b"
            pure (concat [[x], y])
       _ ->
         fail_sys @[Word 8]
 
proc Main() : Main =
  do x <- Simple1()
     y <- Simple2()
     pure Main {x = x, y = y}
